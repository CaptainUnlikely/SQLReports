            <Value>=Parameters!USSLastScanTimeGroup.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>select distinct
CDR.Name,	--Client name
CS.Manufacturer0,	--Computer system manufacturer
CS.Model0,	--Computer system model
CDR.ClientVersion,	--SCCM client version
USS.ScanTime,	--Last completed SUP scan time
USS.LastScanTime,	--Last attempted SUP scan time
USS.ScanPackageVersion,	--WSUS catalog version for last completed scan
USS.LastScanPackageVersion,	--WSUS catalog version for last attempted scan
CASE
	WHEN USS.LastScanState = '0' then 'Scan state unknown'
	WHEN USS.LastScanState = '1' then 'Scan is waiting for catalog location'
	WHEN USS.LastScanState = '2' then 'Scan is running'
	WHEN USS.LastScanState = '3' then 'Scan completed'
	WHEN USS.LastScanState = '4' then 'Scan is pending retry'
	WHEN USS.LastScanState = '5' then 'Scan failed'
	WHEN USS.LastScanState = '6' then 'Scan completed with errors'
END AS WSUSLastScanState,	--Description of last SUP scan state
USS.LastStatusMessageID,	--Status message for last SUP scan, 0 is successful
USS.LastErrorCode,	--Error code for last SUP scan, 0 is successful
USS.LastScanPackageLocation,	--Location WSUS package was last retrieved from - the SUP, the CMG, or blank
USS.LastWUAVersion,	--Windows Update Agent version
CDR.LastDDR,	--Last DDR or heartbeat inventory processed
CDR.LastHardwareScan,	--Last hardware inventory processed
CDR.LastSoftwareScan,	--Last sofwtare (file) inventory processed
CASE
	WHEN CDR.CNIsOnInternet = '0' then 'False'
	WHEN CDR.CNIsOnInternet = '1' then 'True'
	--ELSE 'Unknown'
END as CNIsOnInternet,	--Whether client is on the internet
CDR.LastStatusMessage,	--Last status message processed
CDR.LastActiveTime,	--Last client activity
CDR.LastLogonUser,	--Last logon user
OS.Caption0,	--Operating system full name
OS.LastBootUpTime0,	--Last boot time, to calculate uptime
CDR.DeviceOSBuild,	--Operating system build number
CASE
	WHEN CDR.ClientCheckPass = '1' then 'Passed'
	WHEN CDR.ClientCheckPass = '2' then 'Failed'
	WHEN CDR.ClientCheckPass = '3' then 'No Results'
	ELSE 'Unknown'
END as ClientHealthCheckResult,	--Description of client health check/CCMEval status
CDR.LastClientCheckTime,	--Last CCMEval status time
CHE.HealthCheckDescription,	--Description of CCMEval results
LD.FreeSpace0/1024 as FreeDiskGB,	--Free space on C: drive in GB
CASE
	WHEN PD.HealthStatus0 = '0' then 'Healthy'
	WHEN PD.HealthStatus0 = '1' then 'Warning'
	WHEN PD.HealthStatus0 = '2' then 'Unhealthy'
	ELSE 'Unknown'
END as DiskHealth	--Physical disk health status
from
fn_rbac_CombinedDeviceResources(@UserSIDs) CDR

--Left join these views to include devices which may not have entries in below tables
--For example devices may not have harwdare inventory or may not have run a SUP scan
left join fn_rbac_GS_COMPUTER_SYSTEM(@UserSIDs) CS on CS.ResourceID = CDR.MachineID
left join fn_rbac_UpdateScanStatus(@UserSIDs) USS on USS.ResourceID = CDR.MachineID
left join fn_rbac_GS_LOGICAL_DISK(@UserSIDs) LD on LD.ResourceID = CDR.MachineID
left join fn_rbac_GS_OPERATING_SYSTEM(@UserSIDs) OS on OS.ResourceID = CDR.MachineID
left join fn_rbac_CH_EvalResults(@UserSIDs) CHE on CHE.ResourceID = CDR.MachineID
left join fn_rbac_GS_PHYSICAL_DISK(@UserSIDs) PD on PD.ResourceID = CDR.MachineID

--Exclude devices which we expect to have no records such as the built-in unknown computer objects
where CDR.name not in ('Unknown','x86 Unknown Computer (x86 Unknown Computer)','x64 Unknown Computer (x64 Unknown Computer)','arm64 Unknown Computer (arm64 Unknown Computer)','Provisioning Device (Provisioning Device)')

--Include only C: drive if there's inventory for this
and (LD.ResourceID is NULL or LD.Caption0 like 'C:')
--Everywhere we're using a check for '(All)' is so that we can search for this in parameters to include all results
and (OS.Caption0 = @OSCaption or @OSCaption = '(All)')

--Use "friendly" description for disk health status
and (CAST(
CASE
	WHEN PD.HealthStatus0 = '0' then 'Healthy'
	WHEN PD.HealthStatus0 = '1' then 'Warning'
	WHEN PD.HealthStatus0 = '2' then 'Unhealthy'
	ELSE 'Unknown'
END AS nvarchar) = @PDHealthStatus or @PDHealthStatus = '(All)')

and (CS.Manufacturer0 = @CSManufacturer or @CSManufacturer = '(All)')
and (CS.Model0 = @CSModel or @CSModel = '(All)')

--Allow searching for name instead of selecting from dropdown
and (CDR.Name like ('%' + @Name + '%') or @Name IS NULL)

--Allow searching for name instead of selecting from dropdown
and (CDR.LastLogonUser like ('%' + @LastLogonUser + '%') or @LastLogonUser IS NULL)
and (USS.LastScanPackageLocation = @USSLastScanPackageLocation or @USSLastScanPackageLocation = '(All)')
and (USS.LastWUAVersion = @USSLastWUAVersion or @USSLastWUAVersion = '(All)')
--Use CONVERT where these columns are not strings to standardise them
--CONVERT with 121 is a datetime, CONVERT without is from a bit of integer value
and (CONVERT (varchar, USS.LastScanPackageVersion) = @USSLastScanPackageVersion or @USSLastScanPackageVersion = '(All)')
--and (CONVERT (varchar, USS.ScanTime, 121) = @USSScanTime or @USSScanTime= '(All)')
--and (CONVERT (varchar, USS.LastScanTime, 121) = @USSLastScanTime or @USSLastScanTime= '(All)')
and (CONVERT (varchar, USS.ScanPackageVersion) = @USSScanPackageVersion or @USSScanPackageVersion = '(All)')
and (CONVERT (varchar, USS.LastStatusMessageID) = @USSLastStatusMessageID or @USSLastStatusMessageID = '(All)')
--Use "friendly" description for scan state, and cast to string
and (CAST(
CASE
	WHEN USS.LastScanState = '0' then 'Scan state unknown'
	WHEN USS.LastScanState = '1' then 'Scan is waiting for catalog location'
	WHEN USS.LastScanState = '2' then 'Scan is running'
	WHEN USS.LastScanState = '3' then 'Scan completed'
	WHEN USS.LastScanState = '4' then 'Scan is pending retry'
	WHEN USS.LastScanState = '5' then 'Scan failed'
	WHEN USS.LastScanState = '6' then 'Scan completed with errors'
END AS nvarchar) = @USSLastScanState or @USSLastScanState = '(All)')
and (CONVERT (varchar, USS.LastErrorCode) = @USSLastErrorCode or @USSLastErrorCode = '(All)')
--and (CONVERT (varchar, OS.LastBootUpTime0, 121) = @OSLastBootUpTime or @OSLastBootUpTime = '(All)')
and (CAST(
CASE 
	WHEN DATEDIFF(day, OS.LastBootUpTime0, GETDATE()) &lt;= 1 THEN '1 day old'
	WHEN DATEDIFF(day, OS.LastBootUpTime0, GETDATE()) BETWEEN 2 AND 3 THEN '1–3 days'
	WHEN DATEDIFF(day, OS.LastBootUpTime0, GETDATE()) BETWEEN 4 AND 7 THEN '3–7 days'
	WHEN DATEDIFF(day, OS.LastBootUpTime0, GETDATE()) &gt;7 THEN 'Over 7 days'
END AS nvarchar) = @OSLastBootUpTimeGroup or @OSLastBootUpTimeGroup = '(All)')
--and (CONVERT (varchar, CDR.LastDDR, 121) = @CDRLastDDR or @CDRLastDDR = '(All)')
and (CAST(
CASE 
	WHEN DATEDIFF(day, CDR.LastDDR, GETDATE()) &lt;= 1 THEN '1 day old'
	WHEN DATEDIFF(day, CDR.LastDDR, GETDATE()) BETWEEN 2 AND 3 THEN '1–3 days'
	WHEN DATEDIFF(day, CDR.LastDDR, GETDATE()) BETWEEN 4 AND 7 THEN '3–7 days'
	WHEN DATEDIFF(day, CDR.LastDDR, GETDATE()) &gt;7 THEN 'Over 7 days'
END AS nvarchar) = @CDRLastDDRGroup or @CDRLastDDRGroup = '(All)')
--and (CONVERT (varchar, CDR.LastHardwareScan, 121) = @CDRLastHardwareScan or @CDRLastHardwareScan = '(All)')
and (CAST(
CASE 
	WHEN DATEDIFF(day, CDR.LastHardwareScan, GETDATE()) &lt;= 1 THEN '1 day old'
	WHEN DATEDIFF(day, CDR.LastHardwareScan, GETDATE()) BETWEEN 2 AND 3 THEN '1–3 days'
	WHEN DATEDIFF(day, CDR.LastHardwareScan, GETDATE()) BETWEEN 4 AND 7 THEN '3–7 days'
	WHEN DATEDIFF(day, CDR.LastHardwareScan, GETDATE()) &gt;7 THEN 'Over 7 days'
END AS nvarchar) = @CDRLastHardwareScanGroup or @CDRLastHardwareScanGroup = '(All)')
--and (CONVERT (varchar, CDR.LastSoftwareScan, 121) = @CDRLastSoftwareScan or @CDRLastSoftwareScan = '(All)')
and (CAST(
CASE 
	WHEN DATEDIFF(day, CDR.LastSoftwareScan, GETDATE()) &lt;= 1 THEN '1 day old'
	WHEN DATEDIFF(day, CDR.LastSoftwareScan, GETDATE()) BETWEEN 2 AND 3 THEN '1–3 days'
	WHEN DATEDIFF(day, CDR.LastSoftwareScan, GETDATE()) BETWEEN 4 AND 7 THEN '3–7 days'
	WHEN DATEDIFF(day, CDR.LastSoftwareScan, GETDATE()) &gt;7 THEN 'Over 7 days'
END AS nvarchar) = @CDRLastSoftwareScanGroup or @CDRLastSoftwareScanGroup = '(All)')
--and (CONVERT (varchar, CDR.LastStatusMessage, 121) = @CDRLastStatusMessage or @CDRLastStatusMessage = '(All)')
and (CAST(
CASE 
	WHEN DATEDIFF(day, CDR.LastStatusMessage, GETDATE()) &lt;= 1 THEN '1 day old'
	WHEN DATEDIFF(day, CDR.LastStatusMessage, GETDATE()) BETWEEN 2 AND 3 THEN '1–3 days'
	WHEN DATEDIFF(day, CDR.LastStatusMessage, GETDATE()) BETWEEN 4 AND 7 THEN '3–7 days'
	WHEN DATEDIFF(day, CDR.LastStatusMessage, GETDATE()) &gt;7 THEN 'Over 7 days'
END AS nvarchar) = @CDRLastStatusMessageGroup or @CDRLastStatusMessageGroup = '(All)')
--and (CONVERT (varchar, CDR.LastActiveTime, 121) = @CDRLastActiveTime or @CDRLastActiveTime = '(All)')
and (CAST(
CASE 
	WHEN DATEDIFF(day, CDR.LastActiveTime, GETDATE()) &lt;= 1 THEN '1 day old'
	WHEN DATEDIFF(day, CDR.LastActiveTime, GETDATE()) BETWEEN 2 AND 3 THEN '1–3 days'
	WHEN DATEDIFF(day, CDR.LastActiveTime, GETDATE()) BETWEEN 4 AND 7 THEN '3–7 days'
	WHEN DATEDIFF(day, CDR.LastActiveTime, GETDATE()) &gt;7 THEN 'Over 7 days'
END AS nvarchar) = @CDRLastActiveTimeGroup or @CDRLastActiveTimeGroup = '(All)')
--and (CONVERT (varchar, CDR.LastClientCheckTime, 121) = @CDRLastClientCheckTime or @CDRLastClientCheckTime = '(All)')
and (CAST(
CASE 
	WHEN DATEDIFF(day, CDR.LastClientCheckTime, GETDATE()) &lt;= 1 THEN '1 day old'
	WHEN DATEDIFF(day, CDR.LastClientCheckTime, GETDATE()) BETWEEN 2 AND 3 THEN '1–3 days'
	WHEN DATEDIFF(day, CDR.LastClientCheckTime, GETDATE()) BETWEEN 4 AND 7 THEN '3–7 days'
	WHEN DATEDIFF(day, CDR.LastClientCheckTime, GETDATE()) &gt;7 THEN 'Over 7 days'
END AS nvarchar) = @CDRLastClientCheckTimeGroup or @CDRLastClientCheckTimeGroup = '(All)')
--Use "friendly" description for client check result, and cast to string
and (CAST(
CASE
	WHEN CDR.ClientCheckPass = '1' then 'Passed'
	WHEN CDR.ClientCheckPass = '2' then 'Failed'
	WHEN CDR.ClientCheckPass = '3' then 'No Results'
	--ELSE 'Unknown'
END AS nvarchar) = @CDRClientCheckPass or @CDRClientCheckPass = '(All)')
--Use "friendly" description for whether client is on internet, and cast to string
and (CAST(
CASE
	WHEN CDR.CNIsOnInternet = '0' then 'False'
	WHEN CDR.CNIsOnInternet = '1' then 'True'
	--ELSE 'Unknown'
END AS nvarchar) = @CDRCNIsOnInternet or @CDRCNIsOnInternet = '(All)')
and (CHE.HealthCheckDescription = @CHEHealthCheckDescription or @CHEHealthCheckDescription = '(All)')
and (CDR.ClientVersion = @CDRClientVersion or @CDRClientVersion = '(All)')
--Divide disk free space by 1024 to get space in GB
--and (CONVERT (varchar, LD.FreeSpace0/1024) = @LDFreeDiskGB or @LDFreeDiskGB = '(All)')
and (CAST(
CASE
	when CONVERT (varchar, LD.FreeSpace0/1024) &lt; 1 THEN 'Less than 1GB'
	when CONVERT (varchar, LD.FreeSpace0/1024) BETWEEN 1 AND 5 THEN '1-5GB'
	when CONVERT (varchar, LD.FreeSpace0/1024) BETWEEN 5 AND 10 THEN '5-10GB'
	when CONVERT (varchar, LD.FreeSpace0/1024) BETWEEN 10 AND 20 THEN '10-20GB'
	when CONVERT (varchar, LD.FreeSpace0/1024)  &gt;20 THEN 'Over 20GB'
END AS nvarchar) = @LDFreeDiskGBGroup or @LDFreeDiskGBGroup = '(All)')
--Group the scan time by how many days old it is compared to today
and (CAST(
CASE 
	WHEN DATEDIFF(day, USS.ScanTime, GETDATE()) &lt;= 1 THEN '1 day old'
	WHEN DATEDIFF(day, USS.ScanTime, GETDATE()) BETWEEN 2 AND 3 THEN '1–3 days'
	WHEN DATEDIFF(day, USS.ScanTime, GETDATE()) BETWEEN 4 AND 7 THEN '3–7 days'
	WHEN DATEDIFF(day, USS.ScanTime, GETDATE()) &gt;7 THEN 'Over 7 days'
END AS nvarchar) = @USSScanTimeGroup or @USSScanTimeGroup = '(All)')
and (CAST(
CASE 
	WHEN DATEDIFF(day, USS.LastScanTime, GETDATE()) &lt;= 1 THEN '1 day old'
	WHEN DATEDIFF(day, USS.LastScanTime, GETDATE()) BETWEEN 2 AND 3 THEN '1–3 days'
	WHEN DATEDIFF(day, USS.LastScanTime, GETDATE()) BETWEEN 4 AND 7 THEN '3–7 days'
	WHEN DATEDIFF(day, USS.LastScanTime, GETDATE()) &gt;7 THEN 'Over 7 days'
END AS nvarchar) = @USSLastScanTimeGroup or @USSLastScanTimeGroup = '(All)')
order by CDR.Name</CommandText>
      </Query>
