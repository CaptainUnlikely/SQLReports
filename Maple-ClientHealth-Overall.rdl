        <CommandText>select distinct
COALESCE(USS.LastScanPackageLocation,'(All)') as USSLastScanPackageLocation,
COALESCE(USS.LastWUAVersion,'(All)') as USSLastWUAVersion,
COALESCE(CONVERT (varchar, USS.LastScanPackageVersion),'(All)') as USSLastScanPackageVersion,
--CONVERT (varchar, USS.ScanTime, 121) as USSScanTime,
COALESCE((
CASE 
	WHEN DATEDIFF(day, USS.ScanTime, GETDATE()) &lt;= 1 THEN '1 day old'
	WHEN DATEDIFF(day, USS.ScanTime, GETDATE()) BETWEEN 2 AND 3 THEN '1–3 days'
	WHEN DATEDIFF(day, USS.ScanTime, GETDATE()) BETWEEN 4 AND 7 THEN '3–7 days'
	WHEN DATEDIFF(day, USS.ScanTime, GETDATE()) &gt;7 THEN 'Over 7 days'
END),'(All)') AS USSScanTimeGroup,
--CONVERT (varchar, USS.LastScanTime, 121) as USSLastScanTime,
COALESCE((
CASE 
	WHEN DATEDIFF(day, USS.LastScanTime, GETDATE()) &lt;= 1 THEN '1 day old'
	WHEN DATEDIFF(day, USS.LastScanTime, GETDATE()) BETWEEN 2 AND 3 THEN '1–3 days'
	WHEN DATEDIFF(day, USS.LastScanTime, GETDATE()) BETWEEN 4 AND 7 THEN '3–7 days'
	WHEN DATEDIFF(day, USS.LastScanTime, GETDATE()) &gt;7 THEN 'Over 7 days'
END),'(All)') AS USSLastScanTimeGroup,
COALESCE(CONVERT (varchar, USS.ScanPackageVersion),'(All)') as USSScanPackageVersion,
COALESCE(CONVERT (varchar, USS.LastStatusMessageID),'(All)') as USSLastStatusMessageID,
COALESCE(CONVERT (varchar, USS.LastErrorCode),'(All)') as USSLastErrorCode,
COALESCE((
CASE
	WHEN USS.LastScanState = '0' then 'Scan state unknown'
	WHEN USS.LastScanState = '1' then 'Scan is waiting for catalog location'
	WHEN USS.LastScanState = '2' then 'Scan is running'
	WHEN USS.LastScanState = '3' then 'Scan completed'
	WHEN USS.LastScanState = '4' then 'Scan is pending retry'
	WHEN USS.LastScanState = '5' then 'Scan failed'
	WHEN USS.LastScanState = '6' then 'Scan completed with errors'
END),'(All)') AS USSLastScanState
from fn_rbac_UpdateScanStatus(@UserSIDs) USS
--UNION to bring in null values which are changed to (All) for filtering in parameters
UNION ALL SELECT ISNULL(NULL,'(All)'),
ISNULL(NULL,'(All)'),
ISNULL(NULL,'(All)'),
ISNULL(NULL,'(All)'),
ISNULL(NULL,'(All)'),
ISNULL(NULL,'(All)'),
ISNULL(NULL,'(All)'),
ISNULL(NULL,'(All)'),
ISNULL(NULL,'(All)')
order by 1</CommandText>
