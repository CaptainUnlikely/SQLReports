            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
--CONVERT (varchar, LD.FreeSpace0/1024) as LDFreeDiskGB,
COALESCE((
CASE
	when CONVERT (varchar, LD.FreeSpace0/1024) &lt; 1 THEN 'Less than 1GB'
	when CONVERT (varchar, LD.FreeSpace0/1024) BETWEEN 1 AND 5 THEN '1-5GB'
	when CONVERT (varchar, LD.FreeSpace0/1024) BETWEEN 5 AND 10 THEN '5-10GB'
	when CONVERT (varchar, LD.FreeSpace0/1024) BETWEEN 10 AND 20 THEN '10-20GB'
	when CONVERT (varchar, LD.FreeSpace0/1024)  &gt;20 THEN 'Over 20GB'
END),'(All)') AS LDFreeDiskGBGroup
from fn_rbac_GS_LOGICAL_DISK(@UserSIDs) LD
where (LD.ResourceID is NULL or LD.Caption0 like 'C:')
--UNION to bring in null values which are changed to (All) for filtering in parameters
UNION select ISNULL(NULL,'(All)')
order by 1</CommandText>
      </Query>
