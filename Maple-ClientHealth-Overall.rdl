    <DataSet Name="OSDataSet">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select distinct
COALESCE(OS.Caption0,'(All)') as OSCaption,
COALESCE((
CASE 
	WHEN DATEDIFF(day, OS.LastBootUpTime0, GETDATE()) &lt;= 1 THEN '1 day old'
	WHEN DATEDIFF(day, OS.LastBootUpTime0, GETDATE()) BETWEEN 2 AND 3 THEN '1–3 days'
	WHEN DATEDIFF(day, OS.LastBootUpTime0, GETDATE()) BETWEEN 4 AND 7 THEN '3–7 days'
	WHEN DATEDIFF(day, OS.LastBootUpTime0, GETDATE()) &gt;7 THEN 'Over 7 days'
END),'(All)') AS OSLastBootUpTimeGroup
from fn_rbac_GS_OPERATING_SYSTEM(@UserSIDs) OS
--UNION to bring in null values which are changed to (All) for filtering in parameters
UNION select ISNULL(NULL,'(All)'),
ISNULL(NULL,'(All)') 
order by 1</CommandText>
      </Query>
      <Fields>
        <Field Name="OSCaption">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>OSCaption</DataField>
